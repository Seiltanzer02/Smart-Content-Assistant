# .env.example - Переименуйте в .env и заполните значения

# Ключ для доступа к API OpenRouter (для DeepSeek и других моделей)
OPENROUTER_API_KEY=

# ID и Hash вашего приложения Telegram (получить на my.telegram.org)
TELEGRAM_API_ID=
TELEGRAM_API_HASH=

# Ключ доступа к Unsplash API (получить на unsplash.com/developers)
UNSPLASH_ACCESS_KEY=

# Ключ доступа к Pexels API (получить на pexels.com/api/)
PEXELS_API_KEY=

# Supabase Project URL
SUPABASE_URL=

# Supabase Anon Key (public)
SUPABASE_ANON_KEY= 